<h1>FM Scholars Tracker</h1>
<table class="table">
  <thead>
    <tr>
      <th scope="col">id</th>
      <th scope="col">Discord Name</th>
      <th scope="col">Total SLP</th>
      <th scope="col">Diff</th>
      <th scope="col">Last Tracked At</th>
    </tr>
  </thead>
  <tbody>
    <% @rows.each do |row| %>
      <tr>
        <th scope="row"><%= row.scholar.id %></th>
        <td><%= row.scholar.discord_name %></td>
        <td><%= row.slp_total %></td>
        <td><%= row.slp_delta %></td>
        <td><%= row.last_tracked_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>